{
  "name": "wfm-themes",
  "version": "1.0.0",
  "description": "Allows for building themes & creating user style for stylus",
  "main": "utils/build-themes.js",
  "scripts": {
    "test": "echo \"No tests available\"",
    "build": "node utils/build-themes.js",
    "watch": "node utils/build-themes.js --watch",
    "select": "npm run build && node utils/inject-theme-stylus.js",
    "dev": "npm run build && concurrently \"npm run watch\" \"node utils/inject-theme-stylus.js --watch-only\"",
    "reset": "node -e \"require('fs').unlinkSync('utils/.theme-selection.json')\" && npm run select",
    "new": "node utils/create-theme.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EroiiKZz/wfm-themes.git"
  },
  "keywords": [
    "warframe-market",
    "sass",
    "stylus",
    "warframe",
    "themes",
    "css",
    "compiler",
    "build-tool"
  ],
  "author": "EroiiKZz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EroiiKZz/wfm-themes/issues"
  },
  "homepage": "https://github.com/EroiiKZz/wfm-themes#readme",
  "dependencies": {
    "sass": "^1.77.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
